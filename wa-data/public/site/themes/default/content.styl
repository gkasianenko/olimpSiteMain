.post__portfolio {
  .slick-slider {
    position: relative;
    display: block;
    box-sizing: border-box;
    touch-callout: none;
    user-select: none;
    touch-action: pan-y;
    tap-highlight-color: transparent;

  }
  .slick-list {
    position: relative;
    overflow: hidden;
    display: block;
    margin: 0;
    padding: 0;

    &:focus {
      outline: none;
    }

    &.dragging {
      cursor: pointer;
      cursor: hand;
    }
  }
  .slick-slider .slick-track,
  .slick-slider .slick-list {
    transform: translate3d(0, 0, 0);
  }

  .slick-track {
    position: relative;
    left: 0;
    top: 0;
    display: block;

    &:before,
    &:after {
      content: "";
      display: table;
    }

    &:after {
      clear: both;
    }

    .slick-loading & {
      visibility: hidden;
    }
  }
  .slick-slide {
    float: left;
    height: 100%;
    min-height: 1px;

    img {
      display: block;
    }
    &.slick-loading img {
      display: none;
    }

    //display: none;

    &.dragging img {
      pointer-events: none;
    }

    .slick-initialized & {
      display: block;
    }

    .slick-loading & {
      visibility: hidden;
    }

    .slick-vertical & {
      display: block;
      height: auto;
      border: 0;
      outline: none;

      &:focus,
      &:active,
      &::selection{
        outline: none !important;
        border: 0 !important;
        box-shadow: none;
      }
    }
  }
  .slick-arrow.slick-hidden {
    display: none;
  }

  .slick-current{
    position: relative;
  }


  .slick-slide {
    overflow: hidden;
  }


  .portfolio__container {

  }

  .portfolio__images {
    @media(min-width: 1600px) {
      display: flex
      flex-direction: row;
    }
  }

  .portfolio-image__container {
    margin-left: 50px
    margin-right: 50px

    @media(min-width: 992px) {
      margin-left: 0
      margin-right: 0

      flex: 1 0 auto;
    }
    //width: 700px
  }

  .portfolio-thumbs__container {
    margin-top: 30px
    margin-bottom: 30px

    @media(min-width: 1600px) {
      margin-top: 0
      margin-bottom: 0
    }

    .slick-slider {
      @media(max-width: 991px) {
        &:not(.slick-vertical) {
          margin-left: 40px
          margin-right: 40px
        }
      }

      display: flex
      align-items:center;
      justify-content: center
      position: relative

      @media(min-width: 1600px) {
        flex-direction: column;
      }
    }

    @media(min-width: 1600px) {
      flex 0 0 123px
      margin-left: 0
      margin-right: 22px

      .slick-list {
        width: 123px
      }
      .slick-slider {
        text-align center;
      }
    }


    .slick-slide {
      @media(min-width: 1600px) {
        height: 123px
      }
      //margin-top: 10px
      //margin-bottom: 10px;

      .slide-item {
        box-shadow: 0px 0px 5px 0px #000;
        height: 20vw
        margin-left: 5px
        margin-right: 5px

        @media(min-width: 768px) {
          margin-left: 15px
          margin-right: 15px
          box-shadow: 0px 0px 10px 0px #000;
        }

        @media(min-width: 992px) {
          height: 150px
          margin-top: 10px
          margin-bottom: 10px
        }

        @media(min-width: 1600px) {
          height: 103px
          margin: 10px
        }

        img {
          width: 100%
          height: 100%
          max-width: 100%;
          object-fit: cover;
          object-position: top;

          min-height:100%
        }
      }
    }

    .slick-prev, .slick-next {
      border: none;
      background: transparent;

      i {
        background-repeat: no-repeat;
        background-position: center center;
        background-size: cover;
        display: inline-block;
        width: 11px;
        height: 20px;
      }

      &.slick-prev i {
        background-image: url('img/owl-left.png');
      }
      &.slick-next i {
        background-image: url('img/owl-right.png');
      }
    }

    @media(max-width: 991px) {
      .slick-prev, .slick-next {
        position: absolute;
        width: 40px
        height: 100%
      }
      .slick-prev {
        left: -40px
      }
      .slick-next {
        right: -40px
      }
    }

    @media(min-width: 1600px) {
      .slick-prev {
        transform: rotate(90deg);
      }
      .slick-next {
        transform: rotate(90deg);
      }
    }
  }

  .portfolio-image__container {
    .slick-slider {
      @media(min-width: 992px) {
        width: 100%;
        max-width: 700px
        margin: 0 auto;
      }

      @media(min-width: 1600px) {
        margin: 0;
      }

    }
    .slide-item {
      img {
        width: 100%
        max-width: 100%;
      }
    }
  }

  .section__page-with-nav {
    //@media(max-width: 991px) {
    //  margin-top: 0
    //}

    @media(max-width: 991px) {
      .section__bg--gray {
        background: none
      }
    }
  }

  .section__page-nav {
    width: 100%;
    padding-top: 45px;

    @media (min-width: 992px) {
      width: 39vw
    }
    @media(min-width: 1600px) {
      width: calc(((100vw - 1600px) / 2) + 582px)
    }

    .page-nav__container {
      padding-top: 60px !important;

      @media(min-width: 768px) {
        max-width: none !important;
        min-width: auto !important;
        padding-top: 0px !important
        padding-bottom: 0 !important
      }

      @media (min-width: 992px) {
        padding-top: 0px !important;
        padding-bottom: 40px !important;
        padding-right: 50px !important
        padding-left: 0;
        //padding-left: 40px
      }

      @media (min-width: 1600px) {
        max-width 582px !important
        min-width 492px !important
        padding-right: 105px !important
        padding-top: 0px !important
      }

      h1 {
        font-size: 20px
        line-height: 1.15em
        font-weight:bold;
        margin-bottom: 25px
      }

      .text {
        font-size: 14px
      }

      .portfolio__btn-more {
        margin-top: 50px
      }
    }
  }

  .section__page-content {
    max-width: none;
    padding: 0

    @media(min-width: 992px) {
      padding 45px 40px 80px 40px !important;
    }

    @media(min-width: 1600px) {
      max-width: 950px !important;
    }
  }

  .portfolio__post-nav {
    text-align: right;
    margin-bottom: 18px
    padding-left: 30px
    padding-right: 30px
    margin-right: 25px

    .portfolio__post-prev,
    .portfolio__post-next {
      text-decoration: none
      font-size: 18px;
      font-weight: bold;
      line-height: 22px;
      position relative

      &:hover {
        color: #e30014
      }

      &.portfolio__post-prev {
        &:before {
          background-repeat: no-repeat;
          background-position: center center;
          background-size: cover;
          display: block;
          width: 11px;
          height: 22px;
          content "";
          background-image: url('img/owl-left.png');
          position absolute;
          left -20px;
          top: 0;
        }
      }

      &.portfolio__post-next {
        margin-left: 14px
        border-left: 1px solid #DBDBDB;
        padding-left: 15px

        &:after {
          background-repeat: no-repeat;
          background-position: center center;
          background-size: cover;
          display: block;
          width: 11px;
          height: 22px;
          content "";
          background-image: url('img/owl-right.png');
          position absolute;
          right -20px;
          top: 0;
        }
      }
    }
  }

}


