            span
              font-size: 17px;
              font-weight: 700;

        .post__picture
          width 265px
          height 100%
          margin 35px 25px

          .post__picture__cover
            background #b7b7b7
            transform rotate(7deg)
            max-width 205px
            box-shadow 0 10px 20px 0 rgba(0, 0, 0, 0.25)

            img
              transform rotate(-7deg)

