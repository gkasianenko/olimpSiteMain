            border-color: rgba(56, 117, 187, .32);
            color: #3875bb;
            font-size: 11px;
            font-weight: 400;
            line-height: 1em;
            padding: 5px 10px;
            text-transform: uppercase;


        .post__content
          .post__text
            font-size 20px
            line-height 25px

        &.post__review--video
