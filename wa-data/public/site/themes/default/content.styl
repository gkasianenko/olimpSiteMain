.blog-stream
  & > #post-stream:first-child
    & > a[name^="page_"]
      & + .post__default
        border-top-color: transparent

  section
    &.post
      margin-bottom 20px
      position relative

      a
        color #000
        text-decoration none

        &:hover
          color #000
          text-decoration underline

      .post__image
        height 350px;
        overflow hidden

        @media (min-width 1200px)
          height: 400px

        a
          width 100%

          img
            width 100%
            height: 100%
            object-fit cover
            object-position center

      .post__date
        margin-bottom 10px

      .post__detail
        a
          font-size 26px
          line-height 1em
          color #165fbd
          text-decoration underline

          &:hover
            text-decoration none;

      &.post__default
        padding 30px 0
        margin-bottom: 0

        @media (min-width: 768px)
          padding-top: 40px
          padding-bottom: 40px

        @media (min-width: 992px)
          padding 50px 60px

        @media (min-width: 1200px)
          padding 60px 80px

        &:not(:first-child)
          border-top: 1px solid #e1e1e1;

        @media (min-width: 992px)
          &:hover
            background: #fff;
            border-top-color: #fff;

            & + .post__default
              border-top-color transparent


        .post__title
          h3
            margin-bottom 30px
            font-size 35px
            line-height 1.2em

          a
            display block
            color black
            text-decoration none

        .post__date
          font-size 19px
          line-height: 21px

        .post__text
          font-size 19px
          line-height 29px

      &.post__faq
        background #fff
        text-align left
        //border-bottom 10px solid #d4d7d8
        padding 20px 40px

        .post__title
          h3
            margin-bottom 0

          a
            display block
            padding-right 65px
            line-height 1em
            font-weight bold
            color black
            text-decoration none
            cursor pointer
            font-size: 20px;
            color: #000;

        .post__icon
          .icon
            position absolute
            top 8px
            right 10px
            color black
            width 48px
            height 48px
            border 1px solid #e2e0e0
            border-radius 50%
            display flex
            align-items center
            justify-content center
            text-decoration none
            font-size 18px

            &:before
              content '\f067';

        .post__text
          & > div
            padding-top 20px

            p
              &:last-child
                margin-bottom 0

          &.collapse.show
          &.collapsing
            & + .post__icon
              a
                .icon
                  &:before
                    content '\f00d';

      &.post__portfolio
        padding: 0 9px 18px 9px;
        height: clamp(200px, 50vw, 460px)
        margin-bottom: 0

        @media (min-width: 992px) {
          height: clamp(200px, 30vw, 375px)
        }

        @media (min-width: 1200px) {
          height: clamp(200px, 20vw, 370px)
        }

        .post__image {
          width: 100%;
          height: 100%;

          img {
            width: 100%
            height: 100%
            object-fit: cover;
          }
        }

        .post__content {
          position: absolute;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          display: none;
          color: #fff;
          font-weight: 400;
          background: rgba(35, 48, 95, .9);

          .post__link {
            text-decoration: none;
            color: #fff;
            display: flex;
            align-items: center;
            justify-content: center;
            text-align: center;
            padding: 30px;
            width: 100%;
            height: 100%;
          }


        }

        .post__item {
          position relative;
          width: 100%;
          height: 100%;
          margin-bottom: 0

          &:hover {
            .post__content {
              display: flex;
              flex-direction: column;
              align-items: center;
              justify-content: center;
            }
          }
        }

      &.post__review
        margin-bottom 2rem
        text-align left
        background-color white
        position relative
        border-bottom 1px solid #e1e1e1
        //padding 20px 24px


        .post__title
          h3
            font-size: 20px;
            line-height: 1.25em;
            margin-bottom: 20px;

          a
            text-decoration none

            &:hover
              text-decoration underline

        .post__label

