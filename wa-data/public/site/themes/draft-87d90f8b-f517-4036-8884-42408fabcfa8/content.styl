.blog-stream
  & > #post-stream:first-child
    & > a[name^="page_"]
      & + .post__default
        border-top-color: transparent

  section
    &.post
      margin-bottom 20px
      position relative

      a
        color #000
        text-decoration none

        &:hover
          color #000
          text-decoration underline

      .post__image
        height 350px;
        overflow hidden

        @media(min-width 1200px)
          height: 400px

        a
          width 100%

          img
            width 100%
            height: 100%
            object-fit cover
            object-position center

      .post__date
        margin-bottom 10px

      .post__detail
        a
          font-size 26px
          line-height 1em
          color #165fbd
          text-decoration underline

          &:hover
            text-decoration none;

      &.post__default
        padding 30px 0
        margin-bottom: 0

        @media (min-width: 768px)
          padding-top: 40px
          padding-bottom: 40px

        @media (min-width: 992px)
          padding 50px 60px

        @media (min-width: 1200px)
          padding 60px 80px

        &:not(:first-child)
          border-top: 1px solid #e1e1e1;

        @media (min-width: 992px)
          &:hover
            background: #fff;
            border-top-color: #fff;

            &+.post__default
              border-top-color transparent


        .post__title
          h3
            margin-bottom 30px
            font-size 35px
            line-height 1.2em

          a
            display block
            color black
            text-decoration none

        .post__date
          font-size 19px
          line-height: 21px

        .post__text
          font-size 19px
          line-height 29px

      &.post__faq
        background #fff
        text-align left
        //border-bottom 10px solid #d4d7d8
        padding 20px 40px

        .post__title
          h3
            margin-bottom 0

          a
            display block
            padding-right 65px
            line-height 1em
            font-weight bold
            color black
            text-decoration none
            cursor pointer
            font-size: 20px;
            color: #000;

        .post__icon
          .icon
            position absolute
            top 8px
            right 10px
            color black
            width 48px
            height 48px
            border 1px solid #e2e0e0
            border-radius 50%
            display flex
            align-items center
            justify-content center
            text-decoration none
            font-size 18px

            &:before
              content '\f067';

        .post__text
          & > div
            padding-top 20px

            p
              &:last-child
                margin-bottom 0

          &.collapse.show
          &.collapsing
            & + .post__icon
              a
                .icon
                  &:before
                    content '\f00d';

      &.post__portfolio
        .post__title
          position absolute
          bottom 0
          left 0
          padding 12px
          background rgba(0, 0, 0, 0.72)
          width 100%

          h3
            font-size 18px
            font-weight: 500
            line-height 22px
            margin-bottom 0
            text-decoration none

            a
              color #fff

      &.post__review
        margin-bottom 2rem
        text-align left
        background-color white
        position relative
        border-bottom  1px solid #e1e1e1
        //padding 20px 24px


        .post__title
          h3
            font-size: 20px;
            line-height: 1.25em;
            margin-bottom: 20px;

          a
            text-decoration none

            &:hover
              text-decoration underline

        .post__label

          margin-bottom: 20px

          a.btn
            background: #fff;
            border-color: rgba(56,117,187,.32);
            color: #3875bb;
            font-size: 11px;
            font-weight: 400;
            line-height: 1em;
            padding: 5px 10px;
            text-transform: uppercase;


        .post__content
          .post__text
            font-size 20px
            line-height 25px

        &.post__review--video
          .post__content
            padding 0
            text-align center

            .embed-responsive
              margin-bottom 10px

            span
              font-size: 17px;
              font-weight: 700;

        .post__picture
          width 265px
          height 100%
          margin 35px 25px

          .post__picture__cover
            background #b7b7b7
            transform rotate(7deg)
            max-width 205px
            box-shadow 0 10px 20px 0 rgba(0, 0, 0, 0.25)

            img
              transform rotate(-7deg)

article
  &.post
    font-size 20px

  &.post__portfolio
    .text
      position relative
      font-size 26px

    .portfolio-item__images
      margin-bottom 2em

      img
        margin-bottom 1em

  &.post__review
    .post__content
      position relative
      padding 40px 43px 45px 43px

      .post__text
        position relative
        font-size 26px

      &:before
        content '\f10d'
        font-family: "Font Awesome 5 Free";
        font-weight 900
        color #ffdb60
        font-size 62px
        position absolute
        top 0
        left 0

    .post__cover
      background #b7b7b7
      transform rotate(7deg)
      max-width 450px
      box-shadow 0 10px 20px 0 rgba(0, 0, 0, 0.25)
      margin-top 100px
      margin-bottom 100px

      img
        transform rotate(-7deg)

