.post__portfolio

  .portfolio-image__container
  .portfolio-thumbs__container
    max-width: 800px
    margin: 0 auto

  #portfolio-image
    .item
      padding 10px

    .slide-item
      margin: 0 auto;
      overflow hidden
      box-shadow: 0 0 10px 1px rgba(0,0,0,.4)
      border-radius 4px
      border: 4px solid #fff
      height: 265px

      a
        display flex
        height: 100%

        img
          object-fit cover
          object-position center

      @media(min-width 320px)
        height: 150px

      @media(min-width 380px)
        height: 215px

      @media(min-width 576px)
        height: 470px

      @media(min-width 768px)
        height: 470px

  #portfolio-thumbs
    //.owl-stage
    //  margin 0 auto

    padding-left: 70px
    padding-right: 70px
    position relative

    @media (min-width 768px)
      padding-left: 0
      padding-right: 0

    .item
      padding 10px

    .slide-item
      width: 100%;
      height: 150px;
      object-fit: contain;
      object-position top
      overflow: hidden;
      box-shadow: 0 0 10px 1px rgba(0,0,0,.4)
      border-radius 4px
      border: 4px solid #fff

      img
        cursor pointer
        width: 100% !important
        height: 100% !important
        object-fit cover

    .owl-nav
      .owl-prev
      .owl-next
        border 1px solid #767879
        color #767879
        width 40px
        height 90px
        position absolute
        top 50%
        margin-top -45px
        opacity 1
        border-radius 0
        outline none
        transition background 0.3s

        &:hover
          color #000

        i
          background-repeat: no-repeat;
          background-position: center center;
          background-size cover

          display inline-block
          width 11px
          height 22px

      .owl-next
        right 10px

        @media (min-width 1200px)
          right -70px

        i
          background-image url("img/owl-right.png")

      .owl-prev
        left 10px

        @media (min-width 1200px)
          left: -70px

        i
          background-image url("img/owl-left.png")


